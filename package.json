{
  "name": "csv-to-excel-converter",
  "displayName": "CSV to Excel Converter",
  "description": "Convert CSV files to Excel format with right-click menu - Smart encoding detection, auto-formatting, and seamless integration",
  "version": "0.0.6",
  "publisher": "bigintech",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Data Science",
    "Formatters"
  ],
  "keywords": [
    "csv",
    "excel",
    "xlsx",
    "converter",
    "file conversion",
    "data",
    "spreadsheet",
    "format"
  ],
  "license": "MIT",
  "homepage": "https://github.com/allenlin527/csv-to-excel#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/allenlin527/csv-to-excel.git"
  },
  "bugs": {
    "url": "https://github.com/allenlin527/csv-to-excel/issues"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csvToExcel.convert",
        "title": "Convert to Excel",
        "category": "CSV to Excel"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "csvToExcel.convert",
          "when": "resourceExtname == .csv",
          "group": "2_workspace"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "csvtojson": "^2.0.10",
    "chardet": "^1.6.0",
    "iconv-lite": "^0.6.3"
  }
}